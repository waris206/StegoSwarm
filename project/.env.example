OPENROUTER_API_KEY=your_key_here

# Base URL of the backend server (used for report download links and API-referer header)
# Change this if you deploy the backend somewhere other than localhost
SERVER_URL=http://localhost:5000

# Base URL of the backend as seen by the browser (used by the React frontend)
# Must start with VITE_ so Vite exposes it to client-side code
VITE_API_URL=http://localhost:5000
